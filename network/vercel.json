{
  "builds": [{ "src": "project4/wsgi.py", "use": "@vercel/python" }],
  "routes": [
    { "src": "/static/(.*\\.css)", "dest": "/staticfiles/$1", "headers": { "Content-Type": "text/css" } },
    { "src": "/static/(.*\\.js)", "dest": "/staticfiles/$1", "headers": { "Content-Type": "application/javascript" } },
    { "src": "/(.*)", "dest": "project4/wsgi.py" }
  ],
  "build": { "command": "./build.sh" }
}