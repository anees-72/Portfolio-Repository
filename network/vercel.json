{
  "builds": [
    { "src": "project4/wsgi.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/static/network/(.*\\.js)", "dest": "/public/network/$1", "headers": { "Content-Type": "application/javascript" } },
    { "src": "/static/network/(.*\\.css)", "dest": "/public/network/$1", "headers": { "Content-Type": "text/css" } },
    { "src": "/(.*)", "dest": "project4/wsgi.py" }
  ]
}