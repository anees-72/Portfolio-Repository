{
  "builds": [
    {
      "src": "project4/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/network/(.*\\.css)",
      "dest": "/public/network/$1",
      "headers": {
        "Content-Type": "text/css",
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/static/network/(.*\\.js)",
      "dest": "/public/network/$1",
      "headers": {
        "Content-Type": "application/javascript",
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/(.*)",
      "dest": "project4/wsgi.py"
    }
  ]
}